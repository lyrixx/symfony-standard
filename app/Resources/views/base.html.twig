<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets -%}

<link rel="stylesheet" type="text/css" href="{{
    asset('bundles/sensiotraining/bootstrap/css/bootstrap.css') }}">

        {%- endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block breadcrumb '<a >Homepage</a>' %}
        {% for level in ['success', 'info', 'error', 'warning'] %}
            {% for message in app.session.flashbag.get(level) %}
                <div class="fade in alert alert-{{ level }}"
                    <a class="close" href="#">Ã—</a>
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}

        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
